    <script type="text/template" id="magazineListTemplate">
        <a href="/magazine/{{MG_PK}}">
            <div class="magazine-items">
                <img src="{{MG_THUMBNAIL}}">
                <div class="content-box">
                    <h1 class="subject">{{MG_SUBJECT}}</h1>
                    <p class="content">{{MG_CONTENT}}</p>
                </div>
            </div>
        </a>
    </script>
    <script type="text/template" id="tabTemplate">
        <div class="tab-item" data-value="{{TAB_VALUE}}">
            <span>{{TAB_NAME}}</span>
        </div>
    </script>


